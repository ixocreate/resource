{
    "name": "ixocreate/resource",
    "description": "IXOCREATE Resource",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Resource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Resource\\": "tests/misc/",
            "IxocreateTest\\Resource\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
