{
    "name": "ixocreate/resource-package",
    "description": "IXOCREATE Resource Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/application": "^0.1",
        "ixocreate/database-package": "^0.1",
        "ixocreate/template-package": "^0.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Resource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Resource\\": "tests/misc/",
            "Ixocreate\\Test\\Resource\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
